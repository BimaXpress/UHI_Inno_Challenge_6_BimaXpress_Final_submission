        .sixHeading {
            font-size: 9px;
            font-weight: 600;
            color: black;
        }
        
        .subHeading {
            font-size: 9px;
            letter-spacing: -0.2px;
            font-weight: 500;
            margin-left: 5px;
        }
        
        .subHeading1 {
            font-size: 14px;
            letter-spacing: 0.5px;
            color: white;
            font-weight: 700;
            padding: 1.8px;
        }
        
        .head-div {
            background-color: #006cb5;
            text-align: center;
        }
        
        .customPTag {
            font-size: 13px;
            font-weight: 500;
            display: inline-block;
            letter-spacing: 0.2px;
            margin-left: 25px;
        }
        
        .spanBox {
            font-size: 10px;
            text-align: center;
            display: inline-block;
            border: 0.1px solid black;
            vertical-align: middle;
            line-height: 17px;
            width: 45px;
            height: 20px;
            margin-top: 0px;
            margin-left: -3px;
            overflow: hidden;
        }
        
        .spanBox1 {
            font-size: 8px;
            text-align: center;
            display: inline-block;
            border: 1px solid Black;
            vertical-align: middle;
            line-height: 9px;
            width: 50px;
            height: 17px;
            margin-top: 0;
            margin-left: -4.35px;
        }
        
        .spanBox2 {
            font-size: 8px;
            text-align: center;
            display: inline-block;
            border: 1px solid #444;
            vertical-align: middle;
            line-height: 10px;
            width: 6px;
            height: 6px;
            margin-top: 0px;
            margin-left: -4.35px;
        }
        
        .spanBox4 {
            font-size: 8px;
            text-align: center;
            display: inline-block;
            border: 1px solid #444;
            width: 20px;
            height: 16px;
            margin-top: -17px;
            margin-left: -4.35px;
        }
        
        .spanLine {
            font-size: 10.5px;
            display: inline-block;
            border-bottom: 1px solid #444;
            vertical-align: middle;
            line-height: 0px;
            width: 10px;
            height: 9px;
            margin-top: 0px;
            margin-left: -4.35px;
            text-align: center;
            font-weight: bold;
        }
        
        .customTextBox {
            display: inline-block;
            border: 1px solid #444;
            vertical-align: 11px;
            line-height: 20px;
            height: 20px;
            width: 170px;
            font-size: 12px;
        }
        
        .signatureBox {
            display: inline-block;
            border: 1px solid #444;
            height: 20px;
            width: 170px;
        }
        
        .customMargnBottom4 {
            margin-bottom: -5px;
        }
        
        .customMargnBottom8 {
            margin-bottom: -9px;
        }
        
        .customMargnBottom10 {
            margin-bottom: -5px;
        }
        
        .customMargnBottom15 {
            margin-bottom: -16px;
        }
        
        .nameTextHints {
            font-size: 11px;
            color: #909090;
        }
        
        .claimDocumentSubmittedCheckList {
            font-size: 8px;
            font-weight: 600;
            vertical-align: 30px;
            margin-left: 495px;
            margin-top: 6px;
            /*float: right;*/
        }
        
        .unique {
            margin-top: -5px;
        }
        
        .heading {
            margin-top: 0px;
            padding-top: 0px;
        }
        
        .vl {
            border-left: 12px solid green;
            height: 300px;
        }
        
        .colorbox {
            text-align: center;
            background-color: #c62b27;
            color: white;
            padding-top: 2px;
            padding-bottom: 10px;
            border: 4px solid #9c9998;
            font-size: 14px;
            letter-spacing: 1.3px;
        }
    </style>
    <style type="text/css">
        .tg {
            border-collapse: collapse;
            border-spacing: 0;
        }
        
        .tg td {
            border-color: black;
            border-style: solid;
            border-width: 0.2px;
            font-family: Arial, sans-serif;
            font-size: 8px;
            overflow: hidden;
            padding: 2px 1px;
            word-break: normal;
            border-collapse: collapse;
            text-align: center;
        }
        
        .tg th {
            border-color: black;
            border-style: solid;
            border-width: 0.2px;
            font-family: Arial, sans-serif;
            font-size: 8px;
            font-weight: normal;
            overflow: hidden;
            padding: 1px 1px;
            word-break: normal;
            border-collapse: collapse;
        }
        
        .tg .tg-c3ow {
            border-color: inherit;
            text-align: center;
            vertical-align: middle;
        }
        
        .tg .tg-0pky {
            border-color: inherit;
            text-align: center;
            vertical-align: top;
        }
        
        .float-child {
            width: 49.7%;
            height: 23px;
            float: left;
        }
        
        .border {
            border: 1px solid black;
        }
    </style>
