    <link href="https://fonts.googleapis.com/css2?family=STIX+Two+Text:wght@400;500;600;700&display=swap" rel="stylesheet">
    <style>
        body {
            background-color: #7a7977;
            font-family: 'STIX Two Text', serif;
        }
        
        html {
            margin: 20px 320px 20px 340px;
        }
        
        .invoice {
            width: 760px
        }
        
        .form-container {
            background-color: white;
            padding: 30px;
            padding-top: 0px;
            height: 1040px;
            max-width: 720px;
            position: relative;
            overflow: hidden;
            background-color: white;
            margin: 0px 200px;
            padding-left: 60px;
            padding-right: 25px;
            height: 1050px;
            max-width: 720px;
            margin: auto;
            padding-top: 34px;
            padding-bottom: 24px;
            margin: 0px 0px 0px 0px;
            padding: 0px;
            border: none;
            width: 794px;
            margin: auto;
        }
        
        .border-container {
            border: 3px solid black;
            height: 990px;
        }
        
        #commonContainer {
            font-family: helvatica;
            margin: auto;
            padding: 0px 0px;
            box-sizing: border-box;
            position: relative;
        }
        
        .firstHeading {
            font-size: 14px;
            font-weight: 900;
        }
        
        .secondHeading {
            font-size: 11.5px;
            font-weight: 500;
        }
        
        .thirdHeading {
            font-size: 12px;
